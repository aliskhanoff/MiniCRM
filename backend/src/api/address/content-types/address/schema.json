{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "singularName": "address",
    "pluralName": "addresses",
    "displayName": "Addresses"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Street": {
      "type": "string",
      "required": true
    },
    "PostalCode": {
      "type": "integer"
    },
    "HouseName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "city": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::city.city"
    }
  }
}
